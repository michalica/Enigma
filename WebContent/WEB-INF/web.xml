<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>FileUploadHandler</servlet-name>
    <servlet-class>FileUploadHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileUploadHanlderAs</servlet-name>
    <servlet-class>FileUploadHanlderAs</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegistrationHandlers</servlet-name>
    <servlet-class>RegistrationHandlers</servlet-class>
  </servlet>
   <servlet>
    <servlet-name>LoginHandler</servlet-name>
    <servlet-class>LoginHandler</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Db2</servlet-name>
    <servlet-class>Db2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUploadHandler</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUploadHanlderAs</servlet-name>
    <url-pattern>/uploadAs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Db2</servlet-name>
    <url-pattern>/database</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistrationHandlers</servlet-name>
    <url-pattern>/registrationHandler</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>LoginHandler</servlet-name>
    <url-pattern>/loginHandler</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>